#!/usr/bin/env python

import sys
import argparse

parser = argparse.ArgumentParser()
parser.add_argument('--interval', help='status info every INTERVAL lines', type=int, default=0)
args = parser.parse_args()

count_map = {}
cnt = 0

for line in sys.stdin:
  line = line.strip()
  cnt+=1
  count_map.setdefault(line, 0)
  count_map[line] += 1
  if args.interval > 0 and cnt % args.interval == 0:
    print >> sys.stderr, "line", cnt, "saw ", len(count_map), "different items"

for key in count_map:
  print "%s\t%s" % ( key, count_map[key] )
